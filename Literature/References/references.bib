@unpublished{GutierrezdeRozas2007,
abstract = {The aim of this paper is to assess the level of competition prevailing in the Spanish banking system. The current analysis employs a widely used non-structural methodology put forward by Panzar and Rosse (1987) - the so-called H-statistic - and draws upon a comprehensive panel dataset of Spanish commercial and savings banks covering the period 1986-2005. Standard estimates characterize a hump-shaped profile for the H-statistic throughout the time span under consideration. Nevertheless, a weighted procedure is subsequently performed in order to control for firm size and the number of branches. The estimation outcome reveals a gradual rising path for the H-statistic, thus suggesting a more competitive environment among larger banks. In both settings, a noteworthy increase in the degree of competition is identified at the turn of the eighties, when several liberalization-oriented policy measures came into force. The aforementioned findings discredit the widespread hypothesis which states that concentration impairs competition.},
author = {{Guti{\'{e}}rrez de Rozas}, Luis},
booktitle = {SSRN},
doi = {10.2139/ssrn.1005116},
file = {:Users/Wiktor/Library/Application Support/Mendeley Desktop/Downloaded/Guti{\'{e}}rrez de Rozas - 2007 - Testing for Competition in the Spanish Banking Industry The Panzar-Rosse Approach Revisited.pdf:pdf},
issn = {1556-5068},
keywords = {Banking,Competition,G21,L10,L13,Market Structure,Panzar-Rosse},
title = {{Testing for Competition in the Spanish Banking Industry: The Panzar-Rosse Approach Revisited}},
year = {2007}
}
@article{Harvey2017,
abstract = {Given the competition for top journal space, there is an incentive to produce “significant” results. With the combination of unreported tests, lack of adjustment for multiple tests, and direct and indirect p-hacking, many of the results being published will fail to hold up in the future. In addition, there are basic issues with the interpretation of statistical significance. Increasing thresholds may be necessary, but still may not be sufficient: if the effect being studied is rare, even t {\textgreater} 3 will produce a large number of false positives. Here I explore the meaning and limitations of a p-value. I offer a simple alternative (the minimum Bayes factor). I present guidelines for a robust, transparent research culture in financial economics. Finally, I offer some thoughts on the importance of risk taking (from the perspective of authors and editors) to advance our field.  The transcript and presentation slides are available here: {\textless}a href='http://ssrn.com/abstract=2895842'{\textgreater}http://ssrn.com/abstract=2895842{\textless}/a{\textgreater}.},
author = {Harvey, Campbell R.},
doi = {10.1111/jofi.12530},
file = {:Users/Wiktor/Dropbox/Git/MasterThesis/Literature/All/HARVEY-2017-The{\_}Journal{\_}of{\_}Finance.pdf:pdf},
isbn = {0022-1082},
issn = {15406261},
journal = {Journal of Finance},
number = {4},
pages = {1399--1440},
title = {{Presidential Address: The Scientific Outlook in Financial Economics}},
volume = {72},
year = {2017}
}
@techreport{Gischer,
