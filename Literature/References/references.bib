@article{Mensi2010,
abstract = {This paper explores the use of the Panzar-Rosse statistic as a basis for
empirical assessment of competitive conditions among Tunisian deposit
banks. The elaborated model has been tested with an interest revenues
equation and a total revenues equation. Proceeding by means of an
Ordinary Least Square analysis, the H-statistics is respectively
estimated at 0.87 and 0.91. The computations undertaken using bank fixed
