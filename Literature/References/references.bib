booktitle = {Baltic Journal of Economic Studies},
file = {:Users/Wiktor/Library/Application Support/Mendeley Desktop/Downloaded/Didenko, Bohma - 2016 - RESEARCH OF COMPETITION IN DEPOSIT MARKET OF UKRAINE BASED ON THE PANZAR-ROSSE MODEL.pdf:pdf},
keywords = {G10,G21,H-Staistics Index JEL Classification: C30,Panzar-Rosse model,banking system,competition,deposit market},
number = {2},
title = {{RESEARCH OF COMPETITION IN DEPOSIT MARKET OF UKRAINE BASED ON THE PANZAR-ROSSE MODEL}},
